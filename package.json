{
	"name": "borealis",
	"displayName": "Borealis",
	"description": "Syntax highlighting for the Borealis language",
	"version": "0.1.1",
	"publisher": "usbac",
	"license": "MIT",
	"icon": "images/icon.png",
	"engines": {
		"vscode": "*"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Usbac/vscode-borealis.git"
	},
	"contributes": {
		"languages": [
			{
				"id": "borealis",
				"extensions": [
					".bor"
				],
				"aliases": [
					"Borealis",
					"borealis"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "borealis",
				"scopeName": "source.borealis",
				"path": "./syntaxes/borealis.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "borealis",
				"path": "./snippets/borealis.code-snippets"
			}
		]
	}
}